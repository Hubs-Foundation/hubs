    "lint:html": "htmlhint 'src/**/*.html' && node scripts/indent-linter.js 'src/**/*.html'",
    "lint": "npm run lint:js && npm run lint:html",
    "test": "npm run lint && npm run test:unit && npm run build",
    "test:unit": "ava",
    "stats": "rimraf ./dist && webpack --mode=production --json"
  },
