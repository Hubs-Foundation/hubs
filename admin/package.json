{
  "name": "hubs-admin",
  "version": "0.0.1",
  "description": "Admin console for Hubs.",
  "main": "src/index.js",
  "license": "MPL-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/Hubs-Foundation/hubs.git"
  },
  "scripts": {
    "start": "webpack serve --mode=development --env hubsCloudDev=1",
    "dev": "webpack serve --mode=development --env dev=1",
    "local": "webpack serve --mode=development --env local=1",
    "build": "rimraf ./dist && webpack --mode=production",
    "prettier": "prettier --write '*.js' 'src/**/*.js'",
    "lint:js": "eslint '*.js' 'src/**/*.js'",
    "lint:html": "htmlhint 'src/**/*.html'",
    "lint": "npm run lint:js && npm run lint:html",
    "test": "npm run lint"
  },
  "dependencies": {
    "@iarna/toml": "^2.2.3",
    "@mozilla/lilypad-ui": "^1.8.2",
    "aframe": "github:hubs-foundation/aframe#hubs/master",
    "bitecs": "github:hubs-foundation/bitECS#hubs-patches",
    "classnames": "^2.2.5",
    "hubs": "file:..",
    "react": "^16.1.1",
    "react-admin": "^2.6.3",
    "react-dom": "^16.1.1",
    "react-intl": "^2.4.0",
    "three": "github:hubs-foundation/three.js#hubs-patches-141"
  },
  "devDependencies": {
    "@babel/core": "^7.18.9",
    "@babel/eslint-parser": "^7.18.9",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-object-rest-spread": "^7.18.9",
    "@babel/plugin-proposal-optional-chaining": "7.18.9",
    "@babel/polyfill": "^7.4.4",
    "@babel/preset-env": "^7.18.9",
    "@babel/preset-react": "^7.18.6",
    "@babel/register": "^7.18.9",
    "babel-loader": "^8.2.5",
    "babel-plugin-react-intl": "^3.0.1",
    "babel-plugin-transform-react-jsx-img-import": "^0.1.4",
    "buffer": "^6.0.3",
    "copy-webpack-plugin": "^11.0.0",
    "cors": "^2.8.5",
    "css-loader": "^6.7.1",
    "dotenv": "^16.0.1",
    "eslint": "^8.20.0",
    "eslint-config-prettier": "^8.5.0",
    "esm": "^3.2.25",
    "file-loader": "^6.2.0",
    "html-loader": "^4.1.0",
    "html-webpack-plugin": "^5.5.0",
    "htmlhint": "^1.1.4",
    "mini-css-extract-plugin": "^2.6.1",
    "prettier": "^2.7.1",
    "raw-loader": "^4.0.2",
    "rimraf": "^3.0.2",
    "sass": "^1.54.0",
    "sass-loader": "^13.0.2",
    "selfsigned": "^2.0.1",
    "shelljs": "^0.8.5",
    "style-loader": "^3.3.1",
    "url-loader": "^4.1.1",
    "webpack": "^5.74.0",
    "webpack-cli": "^4.10.0",
    "webpack-dev-server": "^4.9.3",
    "worker-loader": "^3.0.8"
  }
}
